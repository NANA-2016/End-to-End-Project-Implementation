pipeline {
    agent any
    
    stages {
        stage('Checkout SCM') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                sh 'echo "Building Stage"'
            }
        }

        stage('Test') {
            steps {
                sh 'echo "Testing Stage"'
            }
        }
    }
}
